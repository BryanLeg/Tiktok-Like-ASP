@model IEnumerable<TiktokLikeASP.Models.Post>

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
</div>
@foreach (var item in Model)
{
    <article>
        <h1>@item.Title</h1>
        <video>
            <source src="@item.VideoLink)">
        </video>
    </article>
}

